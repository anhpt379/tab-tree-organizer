{
  "manifest_version": 3,
  "name": "Tab Tree Organizer",
  "version": "1.0",
  "description": "Automatically organizes tabs into groups based on their parent-child relationship",
  "permissions": [
    "tabs",
    "tabGroups"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
